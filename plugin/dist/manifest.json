{
  "manifest_version": 3,
  "name": "اشارات",
  "version": "1.0",
  "description": "وصف اشارات وصف اشارات وصف اشارات وصف اشارات",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
